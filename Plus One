 public int[] plusOne(int[] digits) {
    int n = digits.length-1;
    while(n >= 0) {
        
      if (digits[n] == 9) {
        digits[n] = 0;
      }else {
        digits[n]++;
        return digits;
      }
    }
    digits = new int[digits.length + 1];
    digits[0] = 1;
    return digits;
  }
